//прочитать n целых чисел, заменить последние 3 элемента на -1
#include <iostream>
using namespace std;
int main() {
	int n;
	cin >> n;
	int* a = new int[n];
	for (int i = 0; i < n; i++) {
		cin >> a[i];
	}
	for (int i = n-1; i > n - 4; i--) {
		a[i] = -1;
	}
	for (int i = 0; i < n; i++) {
		cout << a[i] << " ";
	}
	return 0;
}
